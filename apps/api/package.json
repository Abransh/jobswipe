{
  "name": "@jobswipe/api",
  "version": "1.0.0",
  "private": true,
  "description": "JobSwipe API server built with Fastify",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:simple": "tsx watch src/minimal-index.ts",
    "dev:minimal": "tsx watch src/minimal-index.ts",
    "build": "tsc --project tsconfig.json",
    "build:easy": "tsc src/index.ts --outDir dist --target ES2020 --module CommonJS --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck --resolveJsonModule --moduleResolution node",
    "build:minimal": "tsc src/minimal-index.ts --outDir dist --target ES2020 --module CommonJS --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck --resolveJsonModule",
    "build:production": "pnpm run clean && pnpm run build",
    "start": "node dist/index.js",
    "start:minimal": "node dist/minimal-index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "db:generate": "cd ../../packages/database && pnpm run db:generate",
    "db:migrate": "cd ../../packages/database && pnpm run db:migrate",
    "db:seed": "cd ../../packages/database && pnpm run db:seed",
    "db:studio": "cd ../../packages/database && pnpm run db:studio",
    "db:reset": "cd ../../packages/database && pnpm run db:reset",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "type-check:minimal": "tsc src/minimal-index.ts --noEmit --skipLibCheck",
    "test": "echo 'Tests not implemented yet'",
    "test:unit": "echo 'Unit tests not implemented yet'",
    "test:integration": "echo 'Integration tests not implemented yet'",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/lib-storage": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.933.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.1.0",
    "@fastify/helmet": "^12.0.1",
    "@fastify/multipart": "^9.3.0",
    "@fastify/oauth2": "^7.8.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/websocket": "^11.2.0",
    "@google/generative-ai": "^0.24.1",
    "@jobswipe/database": "workspace:*",
    "@jobswipe/shared": "workspace:*",
    "@socket.io/redis-adapter": "^8.2.1",
    "@upstash/redis": "^1.35.6",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.4.2",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "fastify-multipart": "^5.4.0",
    "fastify-plugin": "^4.5.1",
    "fastify-zod": "^1.4.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "nanoid": "^3.3.7",
    "openid-client": "^5.6.5",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "socket.io": "^4.7.4",
    "uuid": "^11.1.0",
    "ws": "^8.16.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "fastify",
    "api",
    "typescript",
    "job-automation"
  ]
}